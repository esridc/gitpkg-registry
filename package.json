{
  "version": "0.2.1",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "build:doc": "typedoc src/index.ts",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "preversion": "npm test && npm run build",
    "postversion": "gitpkg publish && git push origin --all; git push origin --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "hub-logger",
  "author": "Haoliang Yu",
  "module": "dist/hub-logger.esm.js",
  "size-limit": [
    {
      "path": "dist/hub-logger.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hub-logger.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/faker": "^5.5.6",
    "@types/node": "^15.12.4",
    "faker": "^5.5.3",
    "gitpkg": "1.0.0-beta.2",
    "husky": "^6.0.0",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typedoc": "^0.20.37",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "winston": "^3.3.3"
  },
  "gitpkg": {
    "registry": "git@github.com:esridc/gitpkg-registry.git"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "6.14.13"
  }
}
