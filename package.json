{
  "version": "0.4.0",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "start": "tsdx watch",
    "build": "tsdx build",
    "build:doc": "typedoc src/index.ts",
    "test": "tsdx test",
    "lint": "tsdx lint src",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "preversion": "npm test && npm run build",
    "postversion": "gitpkg publish && git push origin --all; git push origin --tags",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.ts": "tsdx lint --fix"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "hub-logger",
  "author": "Haoliang Yu",
  "module": "dist/hub-logger.esm.js",
  "size-limit": [
    {
      "path": "dist/hub-logger.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hub-logger.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/split2": "^3.2.1",
    "@types/supertest": "^2.0.11",
    "express": "^4.17.1",
    "gitpkg": "1.0.0-beta.2",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^11.2.6",
    "mockdate": "^3.0.5",
    "size-limit": "^6.0.4",
    "split2": "^4.1.0",
    "supertest": "^6.1.6",
    "tsdx": "^0.7.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "gitpkg": {
    "registry": "git@github.com:esridc/gitpkg-registry.git"
  },
  "volta": {
    "node": "16.13.0",
    "npm": "8.1.2"
  },
  "eslint": {}
}
