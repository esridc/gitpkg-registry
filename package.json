{
  "version": "0.1.4",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "build:doc": "typedoc src/index.ts",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "preversion": "npm test && npm run build",
    "postversion": "gitpkg publish && git push origin --all; git push origin --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "nestjs-hub-logger-service",
  "author": "Haoliang Yu",
  "module": "dist/nestjs-hub-logger-service.esm.js",
  "size-limit": [
    {
      "path": "dist/nestjs-hub-logger-service.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/nestjs-hub-logger-service.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.3",
    "@types/faker": "^5.5.5",
    "faker": "^5.5.3",
    "gitpkg": "1.0.0-beta.2",
    "husky": "^6.0.0",
    "size-limit": "^4.10.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "hub-logger": "git+https://github.com/esridc/gitpkg-registry#hub-logger-v0.2.1-gitpkg"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.17"
  },
  "gitpkg": {
    "registry": "git@github.com:esridc/gitpkg-registry.git"
  },
  "volta": {
    "npm": "7.18.1",
    "node": "16.4.0"
  }
}
